{
  "name": "i-lateral/silverstripe-stripe-forms",
  "description": "Add Stripe subscriptions and payment forms to your Silverstripe website",
    "type": "silverstripe-vendormodule",
    "homepage": "http://github.com/i-lateral/silverstripe-stripe-forms",
    "keywords": ["silverstripe", "payments", "stripe", "forms", "plans", "subscriptions"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "email": "morven@i-lateral.com"
        }
    ],
    "support": {
        "issues": "http://github.com/i-lateral/silverstripe-stripe-forms/issues"
    },
    "require": {
        "silverstripe/framework": "^4.0",
        "stripe/stripe-php": "^7.0"
    },
    "suggest": {
        "silverstripe/cms": "Add full content management support to your site",
        "i-lateral/silverstripe-users": "Add user account management, login and registration"
    },
    "extra": {
        "expose": [
            "javascript"
        ]
    },
	"autoload": {
        "psr-4": {
            "ilateral\\SilverStripe\\StripeForms\\": "src"
        }
    }
}
